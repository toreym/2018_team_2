<h1>Funding Needs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @funding_needs.each do |funding_need| %>
      <tr>
        <td><%= link_to 'Show', funding_need %></td>
        <td><%= link_to 'Edit', edit_funding_need_path(funding_need) %></td>
        <td><%= link_to 'Destroy', funding_need, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Funding Need', new_funding_need_path %>
